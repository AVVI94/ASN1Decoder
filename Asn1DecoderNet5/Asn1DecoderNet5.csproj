<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>
			net5.0;
			net45;
			net461;
			net47;
			net48;
			NETSTANDARD2.1;
			NETSTANDARD2.0;
			NETSTANDARD1.6;
			NETSTANDARD1.5;
			NETSTANDARD1.4;
			NETSTANDARD1.3
		</TargetFrameworks>
		<Product>ASN1DecoderNet5</Product>
		<Company />
		<Version>1.0.8</Version>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<AssemblyVersion>1.0.8.0</AssemblyVersion>
		<FileVersion>1.0.8.0</FileVersion>
		<PackageProjectUrl>https://github.com/AVVI94/ASN1Decoder</PackageProjectUrl>
		<Authors>Jan Král</Authors>
		<Copyright>Copyright © Jan Král</Copyright>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<RepositoryType>Git</RepositoryType>
		<RepositoryUrl>https://github.com/AVVI94/ASN1Decoder</RepositoryUrl>
		<PackageIcon>ASN1Logo.PNG</PackageIcon>
		<PackageIconUrl />
		<PackageTags>certificate, asn1, asn, der, pem, cer, decoder, crypto</PackageTags>
		<PackageReleaseNotes>Added support for more frameworks: net5.0;net45;net461;net47;net48;NETSTANDARD2.1;NETSTANDARD2.0;NETSTANDARD1.6;NETSTANDARD1.5;NETSTANDARD1.4;NETSTANDARD1.3</PackageReleaseNotes>
		<NeutralLanguage>en</NeutralLanguage>
		<Description>Aims to make decoding DER encoded data into ASN1 structure human readable very easy</Description>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<PlatformTarget>AnyCPU</PlatformTarget>
	</PropertyGroup>

	<ItemGroup>
		<Compile Remove="Decoder.old.cs" />
	</ItemGroup>

	<ItemGroup>
		<None Include="..\..\..\..\Desktop\ASN1Logo.PNG">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

</Project>
