<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Product>ASN1DecoderNet5</Product>
    <Company />
    <Version>1.0.5</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyVersion>1.0.4.0</AssemblyVersion>
    <FileVersion>1.0.4.0</FileVersion>
    <PackageProjectUrl>https://github.com/AVVI94/ASN1Decoder</PackageProjectUrl>
    <Authors>Jan Král</Authors>
    <Copyright>Copyright © Jan Král</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryType>Git</RepositoryType>
    <RepositoryUrl>https://github.com/AVVI94/ASN1Decoder</RepositoryUrl>
    <PackageIcon>ASN1Logo.PNG</PackageIcon>
    <PackageIconUrl />
    <PackageTags>certificate, asn1, asn, der, pem, cer, decoder, crypto</PackageTags>
    <PackageReleaseNotes>Description changed</PackageReleaseNotes>
    <NeutralLanguage>en</NeutralLanguage>
    <Description>Aims to make decoding DER encoded data into ASN1 structure human readable very easy</Description>
	  <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Decoder.old.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\..\Desktop\ASN1Logo.PNG">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
